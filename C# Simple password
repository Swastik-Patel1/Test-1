
using System;

class MainClass {
  public static void Main (string[] args) {
    string pw = "40yearOldBot";
    string pw_guess; 

    Console.WriteLine("Enter a password:\n");
    pw_guess = Console.ReadLine();
    
    if (pw_guess == pw) {
      Console.WriteLine("You got it!!!");
    }
    else {
      Console.WriteLine("Nope.");
    }
  }
}
