using System;
using System.Collections.Generic;

// Latest commit 6569b83 on Dec 16, 2020
// How to use a string List in C#
namespace w3Resource_Exercises
{
    public class Randomstuff
    {
        public static void Main(string[] args)
        {
            // Printing nth month

            var monthList = new List<string>() {"January", "February", "March", "April", "May", "June", "July","August", "September", "October", "November", "December"};

            Console.WriteLine("What month do you want?");
            int month = int.Parse(Console.ReadLine());

            Console.WriteLine(monthList[month-1]);
        }
    }
}
